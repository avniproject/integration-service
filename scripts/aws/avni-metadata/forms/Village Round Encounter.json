  "visitScheduleRule" : "\"use strict\";\n({ params, imports }) => {\n  const encounter = params.entity;\n  const moment = imports.moment;\n  const scheduleBuilder = new imports.rulesConfig.VisitScheduleBuilder({encounter});\n  \n  const earliestDate = moment(encounter.earliestVisitDateTime || encounter.encounterDateTime).add(1, 'month').startOf('month').toDate();\n\n  const firstDay = moment(earliestDate).startOf('month').toDate();\n  const lastDay = moment(earliestDate).endOf('month').toDate();\n\n  const earliestDate = moment(firstDay).add(0, 'days').toDate();\n  const maxDate = moment(lastDay).add(0, 'days').toDate();\n  scheduleBuilder.add({name: \"Village Round\", encounterType: \"Village Round\", earliestDate, maxDate});\n  \n  \n  \n  /*let encounterDate = encounter.encounterDateTime; \n  let isChronicDiseaseSuspect = encounter.getObservationValue('88cf5f79-c0d3-4d97-bc87-be34f6a33de0');\n  let isChronicDiseaseSuspectYes = '57e20de7-10de-4391-b7ce-87b2f40d19a2';\n  \n  if(isChronicDiseaseSuspect === isChronicDiseaseSuspectYes){\n    const earliestDate = moment(encounterDate).add(0, 'days').toDate();\n    const maxDate = moment(encounterDate).add(15, 'days').toDate();\n    scheduleBuilder.add({name: \"Referral Communication from VHW\", encounterType: \"Referral Communication from VHW\", earliestDate, maxDate});  \n  }  */\n  \n  return scheduleBuilder.getAll();\n};",
